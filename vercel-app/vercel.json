{
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "https://stickmananimations.replit.app/api/:path*" 
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "silent": false,
    "autoJobCancelation": true
  },
  "trailingSlash": false,
  "cleanUrls": true,
  "framework": "nextjs"
}