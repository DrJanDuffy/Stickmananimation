{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "vercel-entry.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**", "server/**"],
        "nodeVersion": "18.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vercel-entry.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "vercel-entry.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  },
  "github": {
    "silent": true
  }
}
